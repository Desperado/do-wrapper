{
  "name": "do-wrapper",
  "version": "1.1.4",
  "description": "Node.js Wrapper for Digital Ocean API v2",
  "author": "Matt Major",
  "homepage": "https://github.com/matt-major/do-wrapper",
  "repository": {
    "type": "git",
    "url": "https://github.com/matt-major/do-wrapper.git"
  },
  "main": "./dist/do-wrapper.js",
  "scripts": {
    "postinstall": "npm run build && npm run test",
    "build": "tsc src/*.ts --outDir dist/ --module \"commonjs\"",
    "test": "jshint dist/*.js",
    "doc": "./node_modules/.bin/jsdoc dist/*.js -d doc"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matt-major/do-wrapper/issues"
  },
  "keywords": [
    "digital ocean",
    "digitalocean",
    "droplet",
    "cloud",
    "wrapper",
    "api",
    "typescript"
  ],
  "preferGlobal": true,
  "dependencies": {
    "request": "2.55.0"
  },
  "devDependencies": {
    "jsdoc": "^3.3.0-beta3",
    "jshint": "2.6.3"
  }
}
